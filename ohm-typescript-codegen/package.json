{
  "name": "ohm-typescript-codegen",
  "version": "0.1.0",
  "description": "Creates .d.ts files from .ohm grammar files",
  "main": "dist/index.js",
  "repository": "https://github.com/harc/ohm",
  "author": "Patrick Dubroy <pdubroy@gmail.com>",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@ts-morph/bootstrap": "^0.10.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^16.6.1",
    "ava": "^3.15.0",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "fast-glob": "^3.2.7",
    "minimist": "^1.2.5",
    "ohm-js": "^15.5.0"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "yarn build",
    "test": "ava"
  },
  "bin": {
    "generate-ohm-declarations": "bin/generate-ohm-declarations.js"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
