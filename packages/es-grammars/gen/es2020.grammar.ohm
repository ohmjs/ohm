  characterEscape<guardU> = 
    | controlEscape -- alt1
    | "c" controlLetter -- alt2
    | "0" ~decimalDigit -- alt3
    | hexEscapeSequence -- alt4
    | regExpUnicodeEscapeSequence<guardU> -- alt5
