    | "(" "?" ":" disjunction<guardU, guardN> ")" -- alt6

  syntaxCharacter = "\\" | "^" | "$" | "." | "*" | "+" | "?" | "(" | ")" | "[" | "]" | "{" | "}" | "|"

  patternCharacter = 
    | ~syntaxCharacter sourceCharacter

  atomEscape<guardU, guardN> = 
    | decimalEscape -- alt1
    | characterClassEscape<guardU> -- alt2
