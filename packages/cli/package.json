{
  "name": "@ohm-js/cli",
  "type": "module",
  "version": "1.1.0",
  "description": "The official command line tool for ohm-js, a parsing toolkit for JavaScript.",
  "main": "index.js",
  "repository": "https://github.com/harc/ohm",
  "author": "Patrick Dubroy <pdubroy@gmail.com>",
  "license": "MIT",
  "files": [
    "**/*.js"
  ],
  "dependencies": {
    "commander": "^8.1.0",
    "ohm-js": "^16.0.0",
    "fast-glob": "^3.2.7"
  },
  "scripts": {
    "test": "ava"
  },
  "bin": {
    "ohm": "index.js"
  },
  "devDependencies": {
    "ts-morph": "^12.0.0",
    "ava": "^4.3.3",
    "ts-dedent": "^2.2.0",
    "require-from-string": "^2.0.2",
    "@types/require-from-string": "^1.2.1"
  },
  "ava": {
    "extensions": {
      "mjs": true,
      "ts": "module"
    },
    "require": [
      "ts-node/register"
    ],
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
