{
  "name": "typescript-example",
  "version": "1.0.0",
  "description": "An example of using Ohm in a TypeScript project.",
  "private": true,
  "dependencies": {
    "ohm-js": "^16.0.0-pre.2"
  },
  "devDependencies": {
    "@ohm-js/cli": "^0.1.0",
    "@types/node": "^16.9.1",
    "ava": "^3.15.0",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "generate": "ohm generateBundles --withTypes 'src/*.ohm'",
    "test": "yarn generate && ava"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
