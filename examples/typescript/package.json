{
  "name": "typescript-example",
  "version": "1.0.0",
  "description": "An example of using Ohm in a TypeScript project.",
  "type": "module",
  "private": true,
  "dependencies": {
    "ohm-js": "^17.0.0"
  },
  "devDependencies": {
    "@ohm-js/cli": "^2.0.0",
    "@types/node": "^20.19.13",
    "ava": "^6.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3",
    "uvu": "^0.5.6"
  },
  "scripts": {
    "build": "pnpm run generate && tsc --noEmit",
    "generate": "ohm generateBundles --withTypes --esm 'src/*.ohm'",
    "test": "pnpm generate && tsx src/arithmetic.test.ts"
  }
}
